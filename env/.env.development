# 开发环境变量（仅本地开发时生效）
NODE_ENV = "development"  # 标识当前环境为开发环境

# 开发环境接口域名（本地后端服务或测试服务器）
VITE_APP_API_PROTOCOL = "http"
VITE_APP_API_DOMAIN = "localhost:8081"

# 完整接口基础路径（拼接协议和域名）
VITE_APP_API_BASE_URL = "${VITE_APP_API_PROTOCOL}://${VITE_APP_API_DOMAIN}"

# 开发环境特有的调试配置（覆盖.env中的默认值）
VITE_APP_DEBUG = true

# 开发环境静态资源路径（本地服务器）
VITE_APP_STATIC_URL = "/static"

# 开发环境日志级别（详细）
VITE_APP_LOG_LEVEL = "debug"